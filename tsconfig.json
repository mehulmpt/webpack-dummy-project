{
	"compilerOptions": {
	  "module": "esnext",
	  "moduleResolution": "node",
	  "noImplicitAny": false,
	  "removeComments": true,
	  "esModuleInterop": true,
	  "experimentalDecorators": true,
	  "target": "es2017",
	  "baseUrl": ".",
	  "jsx": "react",
	  "skipLibCheck" : true,
	  "sourceMap": true,
	  "inlineSources": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "./build"]
}